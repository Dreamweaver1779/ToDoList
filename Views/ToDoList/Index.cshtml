@model IEnumerable<Notes_ToDoList.Models.ToDoListViewModel>
@{
    ViewBag.Title = "ToDoList";
}

<h2>ToDoList</h2>

@foreach (var item in Model)
{
    <div>
        <h4>@item.Id</h4>
        <h4>@item.TaskName</h4>
        <h4>@item.Description</h4>
        <h4>@item.Priority</h4>
        <h4>@item.Status</h4>
        <h4>@item.Owner</h4>
        <hr />
    </div>
    }

